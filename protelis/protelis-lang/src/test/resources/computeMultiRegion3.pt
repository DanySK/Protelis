import protelis:lang:utils
import protelis:coord:spreading

computeMultiRegion(
	env.get("rootPartition"), 
	(key) -> { key == env.get("rootPartition") },
	() -> { 
		gossip(computeMultiRegion(
					env.get("childPartition"), 
					(key) -> { key < 0.6 },
					() -> { gossip(env.get("n"), min) },
					Infinity
				), min)
	},
	Infinity
)

